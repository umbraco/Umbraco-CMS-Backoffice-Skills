{
	"compilerOptions": {
		"target": "ES2022",
		"module": "ESNext",
		"moduleResolution": "bundler",
		"lib": ["ES2022", "DOM", "DOM.Iterable"],
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"experimentalDecorators": true,
		"useDefineForClassFields": false,
		"declaration": false,
		"noEmit": true
	},
	"include": ["src/**/*.ts"]
}
